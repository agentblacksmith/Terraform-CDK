{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "VisualEditor0",
            "Effect": "Allow",
            "Action": [
                "ec2:CreateNetworkInterface",
                "ec2:DescribeNetworkInterfaces",
                "ec2:DeleteNetworkInterface",
                "dynamodb:ListStreams"
            ],
            "Resource": "*"
        },
        {
            "Sid": "VisualEditor1",
            "Effect": "Allow",
            "Action": [
                "es:ESHttpHead",
                "es:ESHttpPost",
                "dynamodb:GetShardIterator",
                "es:ESHttpGet",
                "dynamodb:DescribeStream",
                "logs:CreateLogGroup",
                "es:ESHttpPut",
                "dynamodb:GetRecords"
            ],
            "Resource": [
                "arn:aws:logs:us-east-1:865227664036:*"

            ]
        },
        {
            "Sid": "VisualEditor2",
            "Effect": "Allow",
            "Action": [
                "logs:CreateLogStream",
                "logs:PutLogEvents"
            ],
            "Resource": "arn:aws:logs:us-east-1:865227664036:log-group:/aws/lambda/stream-handler:*"
        }
    ]
}